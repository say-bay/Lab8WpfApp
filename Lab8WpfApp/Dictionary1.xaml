<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assambly=microlib"
                    xmlns:fontsize="clr-namespace:System.Collections;assambly=microlib"
                    xmlns:fontfamily="clr-namespace:System.Collections;assambly=microlib">
    <fontsize:ArrayList x:Key="size">
        <sys:String>12</sys:String>
        <sys:String>14</sys:String>
        <sys:String>16</sys:String>
    </fontsize:ArrayList>
    <fontfamily:ArrayList x:Key="font">
        <sys:String>Times New Roman</sys:String>
        <sys:String>Arial</sys:String>
        <sys:String>Verdana</sys:String>
    </fontfamily:ArrayList>
</ResourceDictionary>